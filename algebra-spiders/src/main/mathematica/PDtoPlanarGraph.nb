 RowBox[{
  RowBox[{"PDtoPlanarGraph", "[", "K_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "G", ",", "f", ",", "i", ",", "j", ",", "r", ",", "outerFace", ",", 
