    RowBox[{
     RowBox[{"G", "=", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"#", ",", "0"}], "}"}], "&"}], ",", "K", ",", 
        RowBox[{"{", "2", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"f", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"i", "=", 
      RowBox[{"FirstPosition", "[", 
       RowBox[{"G", ",", 
        RowBox[{"{", 
         RowBox[{"_", ",", "0"}], "}"}], ",", 
        RowBox[{"-", "1"}], ",", 
        RowBox[{"{", "2", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"i", "=!=", 
        RowBox[{"-", "1"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"r", "=", 
         RowBox[{"G", "\[LeftDoubleBracket]", 
          RowBox[{"Sequence", "@@", "i"}], "\[RightDoubleBracket]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
           "r", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
           "\[Equal]", "0"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"G", "=", 
            RowBox[{"ReplacePart", "[", 
             RowBox[{"G", ",", 
              RowBox[{
               RowBox[{"Append", "[", 
                RowBox[{"i", ",", "2"}], "]"}], "\[Rule]", "f"}]}], "]"}]}], 
           ";", "\[IndentingNewLine]", 
           RowBox[{"i", "=", 
            RowBox[{
             RowBox[{"FirstPosition", "[", 
              RowBox[{"G", ",", 
               RowBox[{
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{
                  "r", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}],
                   ",", "x_"}], "}"}], "/;", 
                RowBox[{"x", "\[NotEqual]", "f"}]}]}], "]"}], "/.", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"x_", ",", "y_"}], "}"}], "\[Rule]", 
              RowBox[{"{", 
               RowBox[{"x", ",", 
                RowBox[{"Mod", "[", 
                 RowBox[{
                  RowBox[{"y", "-", "1"}], ",", "4", ",", "1"}], "]"}]}], 
               "}"}]}]}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"r", "=", 
            RowBox[{"G", "\[LeftDoubleBracket]", 
             RowBox[{"Sequence", "@@", "i"}], "\[RightDoubleBracket]"}]}], 
           ";"}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
        
        RowBox[{"++", "f"}], ";", "\[IndentingNewLine]", 
        RowBox[{"i", "=", 
         RowBox[{"FirstPosition", "[", 
          RowBox[{"G", ",", 
           RowBox[{"{", 
            RowBox[{"_", ",", "0"}], "}"}], ",", 
           RowBox[{"-", "1"}], ",", 
           RowBox[{"{", "2", "}"}]}], "]"}]}], ";"}]}], "\[IndentingNewLine]",
       "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"G", "=", 
      RowBox[{"Reverse", "[", 
       RowBox[{"G", ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"outerFace", "=", 
      RowBox[{"First", "[", 
       RowBox[{"MaximalBy", "[", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{"--", "f"}], "]"}], ",", 
         RowBox[{
          RowBox[{"Count", "[", 
           RowBox[{
            RowBox[{"List", "@@", 
             RowBox[{"Flatten", "[", 
              RowBox[{"X", "@@", "G"}], "]"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"_", ",", "#"}], "}"}]}], "]"}], "&"}], ",", "1"}], 
        "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"vertexFlags", "=", 
      RowBox[{"StringReplace", "[", 
       RowBox[{
        RowBox[{"ToString", "[", 
         RowBox[{"Join", "[", 
          RowBox[{
           RowBox[{"PD", "[", 
            RowBox[{"X", "[", "]"}], "]"}], ",", "G"}], "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"\"\<PD\>\"", "\[Rule]", "\"\<Vector\>\""}], ",", " ", 
          RowBox[{"\"\<X\>\"", "\[Rule]", "\"\<List\>\""}], ",", " ", 
          RowBox[{
           RowBox[{"\"\<{\>\"", "|", "\"\<[\>\""}], "\[Rule]", "\"\<(\>\""}], 
          ",", " ", 
          RowBox[{
           RowBox[{"\"\<}\>\"", "|", "\"\<]\>\""}], "\[Rule]", 
           "\"\<)\>\""}]}], "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"labels", "=", 
      RowBox[{"StringReplace", "[", 
       RowBox[{
        RowBox[{"ToString", "[", 
         RowBox[{"Vector", "@@", 
          RowBox[{"Table", "[", 
           RowBox[{"\"\<(2,0)\>\"", ",", 
            RowBox[{"Length", "[", "G", "]"}]}], "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"\"\<[\>\"", "\[Rule]", "\"\<(\>\""}], ",", " ", 
          RowBox[{"\"\<]\>\"", "\[Rule]", "\"\<)\>\""}]}], "}"}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"ToString", "[", 
       RowBox[{"StringForm", "[", 
        RowBox[{
        "\"\<PlanarGraph(`1`, `2`, `3`, 0)\>\"", ",", "\[IndentingNewLine]", 
         "outerFace", ",", "vertexFlags", ",", "labels"}], "]"}], "]"}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
